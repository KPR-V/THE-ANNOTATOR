{
  "manifest_version": 3,
  "name": "The Annotator",
  "version": "1.0.0",
  "description": "One stop solution for all your annotating needs",
  "icons": {
    "16": "images/16.png.png",
    "32": "images/32.png.png",
    "48": "images/48.png.png",
    "128": "images/128.png.png"
  },
  "action": {
    "default_popup": "index.html"
  },

  "web_accessible_resources": [
    {
        "resources": ["imported file/jspdf.umd.min.js"],
        "matches": ["<all_urls>"]
    }
],

  "background": {
    "service_worker": "extension's-files/service-worker.js",
    "type": "module"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "scripting",
    "webNavigation",
    "declarativeContent",
    "clipboardRead",
    "clipboardWrite",
    "downloads",
    "unlimitedStorage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["extension's-files/content-script.js"]
    }
  ]
}
